{
  "name": "vueperslides",
  "version": "1.15.0",
  "description": "A touch ready and responsive slideshow for Vue JS.",
  "repository": "https://github.com/antoniandre/vueper-slides",
  "author": "Antoni Andre <antoniandre.web@gmail.com>",
  "license": "MIT",
  "main": "src/index.js",
  "unpkg": "dist/vueperslides.min.js",
  "keywords": [
    "carousel",
    "slideshow",
    "vue",
    "vuejs",
    "ui"
  ],
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "lint": "eslint ./*.js --ext .js,.vue src --cache",
    "build": "npx babel-node --presets=env build.js",
    "build:prod": "NODE_ENV=production npx babel-node --presets=env build.js"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.1",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.3.0",
    "eslint-plugin-html": "^4.0.5",
    "node-sass": "^4.9.3",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-pug": "^0.1.6",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sass": "^0.9.1",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-plugin-vue": "^4.3.2",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 5 version",
    "ie 9",
    "iOS >= 9"
  ]
}
